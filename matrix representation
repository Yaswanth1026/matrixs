#include<stdio.h>
#define MAX 50
int main()
{
    int a[MAX][MAX],i,j,rows,columns;
    printf("Enter the rows and columns of the matrix : ");
    scanf("%d %d",&rows,&columns);
    //scaning input statement
    for(i=0;i<rows;i++)
    {
        for(j=0;j<columns;j++)
        {
            printf("Element - [%d][%d]: ",i,j);
            scanf("%d ",&a[i][j]);
        }
    }
//print statement for the matrix
printf("The matrix is : \n");
for(i=0;i<rows;i++)
{
    for(j=0;j<columns;j++)
    {
        printf("%d",a[i][j]);
    }
    printf("\n");
}
}
